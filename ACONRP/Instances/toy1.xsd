<?xml version="1.0" encoding="utf-8"?>
<xs:schema id="NewDataSet" xmlns="" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
  <xs:element name="Skills">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="Skill" type="xs:string" minOccurs="0" />
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="Pattern">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="PatternEntries" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="PatternEntry" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ShiftType" type="xs:string" minOccurs="0" msdata:Ordinal="0" />
                    <xs:element name="Day" type="xs:string" minOccurs="0" msdata:Ordinal="1" />
                  </xs:sequence>
                  <xs:attribute name="index" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="ID" type="xs:string" />
      <xs:attribute name="weight" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <xs:element name="SchedulingPeriod">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="StartDate" type="xs:string" minOccurs="0" msdata:Ordinal="0" />
        <xs:element name="EndDate" type="xs:string" minOccurs="0" msdata:Ordinal="1" />
        <xs:element name="DayOffRequests" type="xs:string" minOccurs="0" msdata:Ordinal="3" />
        <xs:element name="ShiftOffRequests" type="xs:string" minOccurs="0" msdata:Ordinal="4" />
        <xs:element ref="Skills" minOccurs="0" maxOccurs="unbounded" />
        <xs:element name="ShiftTypes" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Shift" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="StartTime" type="xs:string" minOccurs="0" msdata:Ordinal="0" />
                    <xs:element name="EndTime" type="xs:string" minOccurs="0" msdata:Ordinal="1" />
                    <xs:element name="Description" type="xs:string" minOccurs="0" msdata:Ordinal="2" />
                    <xs:element ref="Skills" minOccurs="0" maxOccurs="unbounded" />
                  </xs:sequence>
                  <xs:attribute name="ID" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Patterns" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element ref="Pattern" minOccurs="0" maxOccurs="unbounded" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Contracts" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Contract" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Description" type="xs:string" minOccurs="0" msdata:Ordinal="0" />
                    <xs:element name="WeekendDefinition" type="xs:string" minOccurs="0" msdata:Ordinal="2" />
                    <xs:element name="SingleAssignmentPerDay" nillable="true" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent msdata:ColumnName="SingleAssignmentPerDay_Text" msdata:Ordinal="1">
                          <xs:extension base="xs:string">
                            <xs:attribute name="weight" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="MaxNumAssignments" nillable="true" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent msdata:ColumnName="MaxNumAssignments_Text" msdata:Ordinal="2">
                          <xs:extension base="xs:string">
                            <xs:attribute name="on" type="xs:string" />
                            <xs:attribute name="weight" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="MinNumAssignments" nillable="true" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent msdata:ColumnName="MinNumAssignments_Text" msdata:Ordinal="2">
                          <xs:extension base="xs:string">
                            <xs:attribute name="on" type="xs:string" />
                            <xs:attribute name="weight" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="MaxConsecutiveWorkingDays" nillable="true" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent msdata:ColumnName="MaxConsecutiveWorkingDays_Text" msdata:Ordinal="2">
                          <xs:extension base="xs:string">
                            <xs:attribute name="on" type="xs:string" />
                            <xs:attribute name="weight" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="MinConsecutiveWorkingDays" nillable="true" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent msdata:ColumnName="MinConsecutiveWorkingDays_Text" msdata:Ordinal="2">
                          <xs:extension base="xs:string">
                            <xs:attribute name="on" type="xs:string" />
                            <xs:attribute name="weight" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="MaxConsecutiveFreeDays" nillable="true" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent msdata:ColumnName="MaxConsecutiveFreeDays_Text" msdata:Ordinal="2">
                          <xs:extension base="xs:string">
                            <xs:attribute name="on" type="xs:string" />
                            <xs:attribute name="weight" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="MinConsecutiveFreeDays" nillable="true" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent msdata:ColumnName="MinConsecutiveFreeDays_Text" msdata:Ordinal="2">
                          <xs:extension base="xs:string">
                            <xs:attribute name="on" type="xs:string" />
                            <xs:attribute name="weight" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="MaxConsecutiveWorkingWeekends" nillable="true" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent msdata:ColumnName="MaxConsecutiveWorkingWeekends_Text" msdata:Ordinal="2">
                          <xs:extension base="xs:string">
                            <xs:attribute name="on" type="xs:string" />
                            <xs:attribute name="weight" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="MinConsecutiveWorkingWeekends" nillable="true" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent msdata:ColumnName="MinConsecutiveWorkingWeekends_Text" msdata:Ordinal="2">
                          <xs:extension base="xs:string">
                            <xs:attribute name="on" type="xs:string" />
                            <xs:attribute name="weight" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="MaxWorkingWeekendsInFourWeeks" nillable="true" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent msdata:ColumnName="MaxWorkingWeekendsInFourWeeks_Text" msdata:Ordinal="2">
                          <xs:extension base="xs:string">
                            <xs:attribute name="on" type="xs:string" />
                            <xs:attribute name="weight" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="CompleteWeekends" nillable="true" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent msdata:ColumnName="CompleteWeekends_Text" msdata:Ordinal="1">
                          <xs:extension base="xs:string">
                            <xs:attribute name="weight" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="IdenticalShiftTypesDuringWeekend" nillable="true" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent msdata:ColumnName="IdenticalShiftTypesDuringWeekend_Text" msdata:Ordinal="1">
                          <xs:extension base="xs:string">
                            <xs:attribute name="weight" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="NoNightShiftBeforeFreeWeekend" nillable="true" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent msdata:ColumnName="NoNightShiftBeforeFreeWeekend_Text" msdata:Ordinal="1">
                          <xs:extension base="xs:string">
                            <xs:attribute name="weight" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="AlternativeSkillCategory" nillable="true" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent msdata:ColumnName="AlternativeSkillCategory_Text" msdata:Ordinal="1">
                          <xs:extension base="xs:string">
                            <xs:attribute name="weight" type="xs:string" />
                          </xs:extension>
                        </xs:simpleContent>
                      </xs:complexType>
                    </xs:element>
                    <xs:element name="UnwantedPatterns" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element ref="Pattern" minOccurs="0" maxOccurs="unbounded" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                  <xs:attribute name="ID" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Employees" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Employee" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ContractID" type="xs:string" minOccurs="0" msdata:Ordinal="0" />
                    <xs:element name="Name" type="xs:string" minOccurs="0" msdata:Ordinal="1" />
                    <xs:element ref="Skills" minOccurs="0" maxOccurs="unbounded" />
                  </xs:sequence>
                  <xs:attribute name="ID" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="CoverRequirements" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="DayOfWeekCover" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Day" type="xs:string" minOccurs="0" />
                    <xs:element name="Cover" minOccurs="0" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Skill" type="xs:string" minOccurs="0" />
                          <xs:element name="Shift" type="xs:string" minOccurs="0" />
                          <xs:element name="Preferred" type="xs:string" minOccurs="0" />
                        </xs:sequence>
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:sequence>
      <xs:attribute name="ID" type="xs:string" />
    </xs:complexType>
  </xs:element>
  <xs:element name="NewDataSet" msdata:IsDataSet="true" msdata:UseCurrentLocale="true">
    <xs:complexType>
      <xs:choice minOccurs="0" maxOccurs="unbounded">
        <xs:element ref="Skills" />
        <xs:element ref="Pattern" />
        <xs:element ref="SchedulingPeriod" />
      </xs:choice>
    </xs:complexType>
  </xs:element>
</xs:schema>